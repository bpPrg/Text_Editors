        'body': '.. toctree::\n   :maxdepth: 4\n\n   $1'

    'Header 1 #':
        'prefix': 'ha'
        'body': '$1\n################################################################################\n\n'

    'Header 2 *':
        'prefix': 'hb'
        'body': '$1\n********************************************************************************\n\n'

