    'body': '\\\\text{$1}\ $0'
  #-----------------------------------------------------------------------------
  # matrix pmatrix bmatrix vmatrix from amsmath
  'matrix':
    'prefix': 'mat'
    'body': '''
    \$\$
    \\\\begin{matrix}
      $1 & $2 \\\\\\\\
      $3 & $4
    \\\\end{matrix}
    \$\$
    '''
  'pmatrix':
    'prefix': 'pmat'
    'body': '''
    \$\$
    \\\\begin{pmatrix}
      $1 & $2 \\\\\\\\
      $3 & $4
    \\\\end{pmatrix}
    \$\$
    '''
  'bmatrix':
    'prefix': 'bmat'
    'body': '''
    \$\$
    \\\\begin{bmatrix}
      $1 & $2 \\\\\\\\
      $3 & $4
    \\\\end{bmatrix}
    \$\$
    '''
  'vmatrix':
    'prefix': 'vmat'
    'body': '''
    \$\$
    \\\\begin{vmatrix}
      $1 & $2 \\\\\\\\
      $3 & $4
    \\\\end{vmatrix}
    \$\$
    '''
  'Vmatrix':
    'prefix': 'Vmat'
    'body': '''
    \$\$
    \\\\begin{Vmatrix}
      $1 & $2 \\\\\\\\
      $3 & $4
    \\\\end{Vmatrix}
    \$\$
    '''
  'bmatrix mn':
    'prefix': 'matmn'
    'body': '''
    \$\$
    \\\\begin{bmatrix}
      x_{11} & x_{12} & x_{13} & \\\\dots & x_{1n} \\\\\\\\
      x_{21} & x_{22} & x_{23} & \\\\dots & x_{2n} \\\\\\\\
      \\\\vdots & \\\\vdots & \\\\vdots & \\\\ddots & \\\\vdots \\\\\\\\
      x_{m1} & x_{m2} & x_{m3} & \\\\dots & x_{mn}
    \\\\end{bmatrix}
    \$\$
    '''

  #-----------------------------------------------------------------------------
  # Astronomy
  'mass of sun':
    'prefix': 'msun'
    'body': 'M_\{\\\\\\odot\}\ '
  'mass of sun dollar enclosed':
    'prefix': 'xmsun'
    'body': '\$M_\{\\\\\\odot\}\$'
  'radius of sun':
    'prefix': 'rsun'
    'body': 'R_\{\\\\\\odot\}\ '
  'radius of sun dollar enclosed':
    'prefix': 'xrsun'
    'body': '\$R_\{\\\\\\odot\}\$'
  #-----------------------------------------------------------------------------
  # Environments
  'Figure Environement':
    'prefix': 'fig'
    'body': '''
    \\\\begin{figure}[ht!]
        \\\\centering
        \\\\includegraphics[width=0.5\\\\textwidth]{${1:only_basename}}
        \\\\caption[$2]{$2}
        \\\\label{[fig:$1]}
    \\\\end{figure}
    $0
    '''
  'Figure Environement With Citation':
    'prefix': 'figg'
    'body': '''
    \\\\begin{figure}[ht!]
        \\\\centering
        \\\\includegraphics[width=0.5\\\\textwidth]{${1:only_basename}}
        \\\\caption[$2]{$2 ~\\\\protect\\\\cite{$3}}
        \\\\label{[fig:$1]}
    \\\\end{figure}
    $0
    '''
  'Easy List & &&':
    'prefix': 'elst'
    'body': '''
            \\\\begin{easylist}

              & {$1}
              & {$2}
              & {$3}
            \\\\end{easylist}
            '''
  'Align(ed)':
    'prefix': 'ali'
    'body': '''
      \\\\begin{align}\\\\label{[eq:$1]}
        $2 & = $3 \\\\\\\\
        $4  & = $5 \\\\nonumber \\\\\\\\
        $6  & = $7 \\\\nonumber
      \\\\end{align}
      $0
    '''
  'Align Equation':
    'prefix': 'eqa'
    'body': '''
      \\\\begin{align}\\\\label{[eq:$1]}
        $2 & = $3 \\\\\\\\
        $4  & = $6 \\\\nonumber \\\\\\\\
        $7  & = $8 \\\\nonumber
      \\\\end{align}
      $0
    '''
  'Description':
    'prefix': 'desc'
    'body': '\\\\begin{description}\n\t\\\\item[$1]$2\n\\\\end{description}'
  'Enumerate':
    'prefix': 'enum'
    'body': '\\\\begin{enumerate}\n\t\\\\item $1\n\\\\end{enumerate}'
  'Itemize':
    'prefix': 'item'
    'body': '\\\\begin{itemize}\n\t\\\\item $1\n\\\\end{itemize}'
  'Split':
    'prefix': 'spl'
    'body': '\\\\begin{split}\n\t$0\n\\\\end{split}'
  'Equation':
    'prefix': 'eq'
    'body': '\\\\begin{eqnarray}\\\\label{[eq:$1]}\n\t$2\n\\\\end{eqnarray}'
  'Equation Aligned':
    'prefix': 'eqa'
    'body': '''
    \\\\begin{eqnarray}\\\\label{[eq:$1]}
        $2 &=& $3 \\\\\\\\
        $4 &=& $5 \\\\nonumber
    \\\\end{eqnarray}
    '''
  'Equation Alignedd':
    'prefix': 'eqaa'
    'body': '''
    \\\\begin{eqnarray}\\\\label{[eq:$1]}
        $2 &=& $3 \\\\\\\\
        $4 &=& $5 \\\\nonumber \\\\\\\\
        $6 &=& $7 \\\\nonumber
    \\\\end{eqnarray}
    '''
  'Gather(ed)':
      'prefix': 'gat'
      'body': '\\begin{gather}\n\t$0\n\\end{gather}'
  'Cases':
      'prefix': 'cas'
      'body': '\\begin{cases}\n\t${1:equation}, &\\text{ if }${2:case}\\\\\n\t$0\n\\end{cases}'
  #-----------------------------------------------------------------------------
  # Sections
  'Part':
    'prefix': 'part'
    'body': '''
    \\paragraph{${1:part name}}\n\\label{part:${1:part label}}\n$2
    '''
  'Chapter':
    'prefix': 'cha'
    'body': '''
    \\chapter{${1:chapter name}}\n\\label{chap:${1:chapter label}}\n$2
    '''
  'Section':
    'prefix': 'sec'
    'body': '''
    %
    %
    %#******************************************************************************
    %#==============================================================================
    %#          Section : $1
    %#==============================================================================
    %#******************************************************************************
    %
    %
    \\\\\\section{$1}\\\\label{sec:$2}
    '''
  'Sub Section':
    'prefix': 'sub'
    'body': '''
    %
    %
    %#------------------------------------------------------------------------------
    %#          Subsection 1a: $1
    %#------------------------------------------------------------------------------
    %
    %
    \\\\\\subsection{$1}\\\\\\label{subsec:$2}\n$0
    '''
  'Sub Sub Section':
    'prefix': 'subsub'
    'body': '''
    %
    %
    %```````````````````````````````````````````````````````````````````````````````
    %         Subsubsection : ${1:subsubsection name}
    %```````````````````````````````````````````````````````````````````````````````
    %
    \\\\subsubsection{$1}\\\\label{subsusb:${2}}\n$0
    '''
  'Paragraph':
    'prefix': 'para'
    'body': '''
    \\paragraph{${1:paragraph name}}\n\\label{par:${1:paragraph label}}\n$2
    '''
  'Sub Paragraph':
    'prefix': 'subp'
    'body': '''
    \\subparagraph{${1:subparagraph name}}\n\\label{subp:${1:subparagraph label}}\n$2
    '''
