  'Beamer Frame':
    'prefix': 'bmfr'
    'body': '''
    %
    %
    %#------------------------------------------------------------------------------
    %#          Frame: $1
    %#------------------------------------------------------------------------------
    %
    %
    \\\\begin{frame}[fragile]{$1}
      $2
    \\\\end{frame}
    '''
