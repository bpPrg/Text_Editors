    'prefix': 'bmeqh'
    'body': '''
    \\\\begin{frame}
      \\\\frametitle{$1}
      \\\\begin{align}
        \\\\colorbox{blue!20}{$ x^2 + y^2$} &= 4
        \\\\end{align}
    \\\\end{frame}
    $0
    '''
