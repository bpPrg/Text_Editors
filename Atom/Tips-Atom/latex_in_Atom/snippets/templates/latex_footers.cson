#------------------------------------------------------------
# For package [acronym]{glossaries}
#------------------------------------------------------------
  'acronym':
    'prefix': 'acro'
    'body': '\\\\newacronym{$1}{$2}{$3}'
  'glossary':
    'prefix': 'glos'
    'body': '''
    \\\\newglossaryentry{$1}{
    name=$2,
    description={$3}}
    '''
  'glossary for equation':
    'prefix': 'glose'
    'body': '''
    \\\\newglossaryentry{$1}{
    name=$2,
    description={\$\$
    $3
    \$\$}}
    '''


# Additional Latex snippets
  'a1':
    'prefix': 'da'
    'body': 'deflection angle '
  'a2':
    'prefix': 'ea'
    'body': 'Einstein angle '
  'a3':
    'prefix': 'gl'
    'body': 'gravitational lensing '
  '1/2':
    'prefix': 'hlf'
    'body': '\\\\frac{1}{2}\ $0'
  'Dollar Signs':
    'prefix': 'dl'
    'body': '\$$1\$'
  'Dollar Signs With Space':
    'prefix': 'dll'
    'body': '\$$1\$\ $0'
  'Equivalent Sign':
    'prefix': 'eqv'
    'body': '\\\\equiv'
  'Definition':
    'prefix': 'def'
    'body': '''
    \\\\begin{equation}\\\\label{[eq:$1]}
      \\\\boxed{ $2 \\\\equiv $3 } \\\\quad (\\\\text{$4})
    \\\\end{equation}
    '''
  'Definition Added Later':
    'prefix': 'deff'
    'body': '\\\\quad (\\\\text{$1})'
  'Modulus':
    'prefix': 'mod'
    'body': '\\\\lvert $1 \\\\rvert'
  #------------------------------------------------------------
  # Latex Typo
